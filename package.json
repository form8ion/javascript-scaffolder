{
  "name": "@travi/javascript-scaffolder",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "description": "opinionated scaffolder for JavaScript projects",
  "license": "MIT",
  "repository": "travi/javascript-scaffolder",
  "bugs": "https://github.com/travi/javascript-scaffolder/issues",
  "homepage": "https://npm.im/@travi/javascript-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:md": "remark . --frail --quiet",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:engines": "check-engine",
    "lint:peer": "npm ls --production >/dev/null",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "pretest:integration": "preview",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "pregenerate:md": "run-s build:js",
    "generate:md": "remark . --output",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "lib/",
    "templates/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.10.5",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@travi/any": "2.0.10",
    "@travi/babel-preset": "3.0.27",
    "@travi/eslint-config": "1.0.44",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-mocha": "1.0.8",
    "@travi/project-scaffolder": "8.10.4",
    "@travi/travis-lint": "1.0.1",
    "@travi/travis-scaffolder-javascript": "3.2.2",
    "ban-sensitive-files": "1.9.8",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "check-engine": "1.8.1",
    "codecov": "3.7.2",
    "commitlint-config-travi": "1.3.8",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.2.0",
    "gherkin-lint": "4.1.2",
    "husky": "4.2.5",
    "lockfile-lint": "4.3.7",
    "mocha": "8.1.1",
    "mock-fs": "4.12.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "package-preview": "3.0.0",
    "remark-cli": "8.0.1",
    "remark-preset-lint-travi": "1.3.9",
    "remark-toc": "7.0.0",
    "remark-usage": "8.0.0",
    "rollup": "2.23.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "9.0.2",
    "testdouble": "3.16.1"
  },
  "dependencies": {
    "@form8ion/core": "1.0.0",
    "@form8ion/javascript-core": "2.0.0",
    "@form8ion/overridable-prompts": "^1.0.0",
    "@hapi/hoek": "^9.0.3",
    "@hapi/joi": "^17.1.0",
    "@travi/cli-messages": "^1.0.1",
    "@travi/language-scaffolder-prompts": "1.1.1",
    "camelcase": "^6.0.0",
    "deepmerge": "^4.2.2",
    "execa": "^4.0.0",
    "inquirer": "^7.0.0",
    "lodash.uniq": "4.5.0",
    "make-dir": "3.1.0",
    "mustache": "^4.0.0",
    "npm-conf": "1.1.3",
    "shelljs": "^0.8.3",
    "snyk": "^1.360.0",
    "touch": "^3.1.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "snyk": true
}
